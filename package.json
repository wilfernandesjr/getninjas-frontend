{
  "name": "dynamic-form",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev:assets": "webpack --watch",
    "dev:start": "live-server --open=./public/ --host=localhost --watch=./public/",
    "dev": "npm-run-all -p dev:*",
    "cypress": "yarn dev & wait-on http://localhost:8080; cypress run",
    "test": "node_modules/jest/bin/jest.js; yarn run cypress",
    "test:watch": "node_modules/jest/bin/jest.js test --watch",
    "standard": "node_modules/standard/bin/cmd.js src/",
    "build": "cross-env NODE_ENV=production webpack"
  },
  "license": "MIT",
  "standard": {
    "env": [
      "jest",
      "customElements",
      "HTMLElement"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": ["/node_modules/", "/cypress/"]
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "cypress": "^3.4.1",
    "file-loader": "^4.2.0",
    "jest": "^24.9.0",
    "lit-html": "^1.1.2",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^8.0.0",
    "standard": "^14.1.0",
    "tagged-template-noop": "^2.1.0",
    "wait-on": "^3.3.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0"
  }
}
